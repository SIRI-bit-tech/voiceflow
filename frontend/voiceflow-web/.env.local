# VoiceFlow CMS Frontend Environment Variables

# API Configuration
NG_APP_API_URL=http://localhost:8000
NG_APP_API_BASE_URL=http://localhost:8000/api

# Application Settings
NG_APP_APP_NAME=VoiceFlow CMS
NG_APP_VERSION=1.0.0

# Development Settings
NG_APP_ENVIRONMENT=development
NG_APP_DEBUG=true

# WebSocket Configuration
NG_APP_WS_URL=ws://localhost:8000/ws

# Feature Flags
NG_APP_ENABLE_VOICE_FEATURES=true
NG_APP_ENABLE_SPATIAL_AUDIO=true
NG_APP_ENABLE_REAL_TIME_COLLABORATION=true

# Analytics (optional)
NG_APP_ANALYTICS_ENABLED=false
NG_APP_ANALYTICS_ID=

# Cloudinary Configuration (for media uploads)
NG_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
NG_APP_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Voice Processing Settings
NG_APP_WHISPER_ENDPOINT=http://localhost:8000/api/voice/stt
NG_APP_TTS_ENDPOINT=http://localhost:8000/api/voice/tts

# UI Configuration
NG_APP_THEME=default
NG_APP_LANGUAGE=en
NG_APP_TIMEZONE=UTC

# Security Settings
NG_APP_ENABLE_HTTPS=false
NG_APP_SESSION_TIMEOUT=3600

# Performance Settings
NG_APP_CACHE_ENABLED=true
NG_APP_CACHE_DURATION=300